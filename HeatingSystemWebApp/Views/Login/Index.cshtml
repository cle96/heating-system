@model HeatingSystemAdministration.Meter
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <div>
        @using (Html.BeginForm())
        {
            <div>
                <div>@Html.LabelFor(m => m.Id)</div>
                <div>@Html.EditorFor(m => m.Id)</div>
                <div style="color:red">@ViewBag.ErrorMessage</div>
                <input type="submit" value="Login" />
            </div>
        }
    </div>
</body>
</html>
