<Window x:Class="HeatingSystemAdministration.Forms.Statistics"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HeatingSystemAdministration.Forms"
        mc:Ignorable="d"
        Title="Statistics" Height="125" Width="300">
    <Grid>
        <StackPanel>
            <Label x:Name="lblTotalConsumption"></Label>
            <Label x:Name="lblMoneySpent"></Label>
            <Label x:Name="lblPercentage"></Label>
        </StackPanel>
    </Grid>
</Window>
